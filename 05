#5-1
a <- list()
for (i in c(0:9)){
  t <- c()
  for(j in c(0 : 9)){
    num <- 10*i+j
    if(sqrt(num) %% 1 == 0)
    t <- c(t,num)
  }
  a <- c(a,list(t))
}
a
